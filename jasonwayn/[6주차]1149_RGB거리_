def func(n,l1,result):
    l2=l1[n-1]
    result.append([])
    for i in range(n):
        if i!=0:
            result[n-1][i]+l2[0]
    result.append([result[n-1][1]+l2[0],result[n-1][2]+l2[0]],[result[n-1][0]+l2[1],result[n-1][2]+l2[1]],[result[n-1][0]+l2[2],result[n-1][1]+l2[2]])





n=int(input())
l1=[]
result=[]
for _ in range(n):
    a,b,c=map(int,input().split())
    l1.append([a,b,c])
    result.append([[a],[b],[c]])


